APP_ENV=local
APP_TZ=Asia/Jakarta
APP_PORT=:8080
HTTP_TIMEOUT=30

# DATABASE
DATABASE_DRIVER=postgres #mysql
# DATABASE_CONNECTION_STRING=USERNAME:PASSWORD@tcp(HOST:PORT)/DB_NAME?parseTime=true # MySQL
DATABASE_CONNECTION_STRING=user=USERNAME password=PASSWORD host=HOST port=PORT dbname=DB_NAME sslmode=disable # PostgreSQL
DATABASE_MAX_OPEN_CONNECTIONS=100
DATABASE_MAX_IDLE_CONNECTIONS=10